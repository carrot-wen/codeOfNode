{
  //默认全局变量
  "globals": {
    "$": false,
    "window": false,
    "document": false,
    "console": false,
    "setTimeout": false,
    "process": false
  },
  //严格模式
  "strict": true,
  //循环必须加花括号
  "curly": true,
  //不能用 == 和 !=
  "eqeqeq": true,
  //禁止使用 arguments.caller和 arguments.callee
  "noarg": true,
  //不允许出现空代码块
  "noempty": true,
  //允许在if，for，while语句中使用赋值
  "boss": false,
  //"quotmark": "single",
  //提示变量 未定义
  "undef": true,
  //变量定义了未使用
  "unused": true,
  //可以使用es6语法
  "esversion": 6,
  //避免function form of "use strict". (W097) 不知道为什么
  "node": true
}